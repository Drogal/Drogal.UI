{
  "name": "@drogal/drogal-ui",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "login": "npm login --registry=https://npm.pkg.github.com",
    "release": "microbundle-crl --no-compress --no-css-modules --format modern,cjs && standard-version && git push --follow-tags && npm publish --access public",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "build": "microbundle-crl --no-compress --no-css-modules --format modern,cjs",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 9001 -c .storyconfig",
    "storybook_mac": "NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 9001 -c .storyconfig"
  },
